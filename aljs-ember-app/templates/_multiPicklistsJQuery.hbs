var idsToSelect = ['option-1', 'option-2'];
var idsToUnselect = ['option-3', 'option-4'];

$('[data-aljs="multi-picklist"]').multiPicklist('setSelectedItems', idsToSelect);
$('[data-aljs="multi-picklist"]').multiPicklist('setUnselectedItems', idsToUnselect);
$('[data-aljs="multi-picklist"]').multiPicklist('getSelectedItems');
$('[data-aljs="multi-picklist"]').multiPicklist('getUnselectedItems');
